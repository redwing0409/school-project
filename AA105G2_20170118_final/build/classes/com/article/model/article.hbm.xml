<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.article.model.ArticleVO" table="article" >
        
        <id name="article_no" column="article_no">
            <generator class="sequence" >
              <param name="sequence">article_seq</param>
            </generator>  
        </id>
        
        <property name="member_no" column="member_no" />
        <property name="article_title" column="article_title" />
        <property name="article_content" column="article_content" />
        <property name="article_views" column="article_views" />
        <property name="article_time" column="article_time" />
        <property name="article_status" column="article_status" />
        
    </class>
</hibernate-mapping>